    # sh "Мои ножки..."
    sh "Ayaklarım..."

# game/EPII/0.2.1/sheila_ep2_1.rpy:575
translate turkish cabin_feet_a6401638:

    # gg "{i}Ее ноги так сексуальны в этих чулках...{i}"
    gg "{i}Bacakları bu çoraplarla çok seksi...{/i}"

# game/EPII/0.2.1/sheila_ep2_1.rpy:582
translate turkish cabin_feet_8f77f6a3:

    # gg "{i}Нейлон чуть скользит по члену, но она все еще сжимает его так крепко...{/i}"
    gg "{i}Naylon sikime biraz sürtünüyor ama yine de onu çok sıkı sıkıyor...{/i}"

# game/EPII/0.2.1/sheila_ep2_1.rpy:583
translate turkish cabin_feet_35604dc3:

    # gg "{i}Черт, как быстро она заняла нужную позицию, я даже не успел ничего сказать...{/i}"
    gg "{i}Kahretsin, doğru pozisyona ne kadar çabuk girdi, bir şey söyleme fırsatım bile olmadı...{/i}"

# game/EPII/0.2.1/sheila_ep2_1.rpy:584
translate turkish cabin_feet_5aa3cb46:

    # gg "{i}Ка же удачно она легла, я вижу и ножки, и грудь и даже часть попы.{/i}"
    gg "{i}O kadar güzel uzandı ki, bacaklarını, göğsünü ve hatta poposunun bir kısmını görebiliyorum.{/i}"

# game/EPII/0.2.1/sheila_ep2_1.rpy:586
