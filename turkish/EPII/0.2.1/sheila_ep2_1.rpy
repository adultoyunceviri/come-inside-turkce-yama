# game/EPII/0.2.1/sheila_ep2_1.rpy:559
translate turkish cabin_mouth_2_039ed9e7:

    # sh "Ммм..."
    sh "Mmm..."

# game/EPII/0.2.1/sheila_ep2_1.rpy:561
translate turkish cabin_mouth_2_717d3a15:

    # sh "Она такая вкусная... Я думаю мне стоит чаще ее пробовать в будущем."
    sh "Nefis! Çok lezzetli... Bence bunu gelecekte daha çok denemeliyim." #edited_rd1

# game/EPII/0.2.1/sheila_ep2_1.rpy:562
translate turkish cabin_mouth_2_19fe303e:

    # sh "Надо собрать остатки с лица, не должно пропасть ни одной капли..."
    sh "Şimdi geri kalanını yüzümden almalıyım, tek bir damla bile bırakmamalıyım..." #edited_rd1

# game/EPII/0.2.1/sheila_ep2_1.rpy:573
